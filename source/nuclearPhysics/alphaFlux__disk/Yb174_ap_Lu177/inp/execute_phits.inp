     ne =  300
   emin =  0.0
   emax =  60.0
   part =  alpha
   unit =    2           $$ unit is [1/cm^2/MeV/source]
   axis =  eng
   file =  out/fluence_energy.dat
  x-txt =  energy [MeV]
  y-txt =  track length [alphas m/MeV/s]
 epsout =    1


